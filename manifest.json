{
  "manifest_version": 2,
  "version": "1.4",
  "name": "passhub.net",
  "description": "username/password and bank card data automatic form filler",
  "icons": {
    "48": "images/favicon_2d.svg"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "passhub-autofill-v1@wwpass.com"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "32": "images/favicon_2d.svg",
      "64": "images/favicon_2d.svg"
    },
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "webNavigation",
    "scripting",
    "https://*/"
  ]
}